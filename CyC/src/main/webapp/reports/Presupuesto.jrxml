	<queryString>
		<![CDATA[select
    p.*,
    pl.*,
    cl.*,
    pl.cantidad * pl.precio_unitario as importe
from
    presupuesto p
    inner join presupuesto_linea pl
        on (p.id = pl.presupuesto)
    left join cliente cl
        on (p.cliente = cl.id)
where
    p.id = $P{PresupuestoId}
order by
    pl.id]]>
	</queryString>
